{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "react/destructuring-assignment": [2, "always", { "ignoreClassFields": true }],
        "react/prop-types": 0,
        "react/jsx-filename-extension": 0,
        "no-return-assign": 0
    }
}
